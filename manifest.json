{
  "name": "Keep Mediuming For Free",
  "version": "1.0",
  "description": "Remove identifier cookie from sent headers when in medium.com sites",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "webRequest",
    "webRequestBlocking",
    "*://medium.com/*",
    "*://*.medium.com/*"
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {},
  "icons": { "16": "icons/m.png", "128": "icons/mbig.png" }
}
